<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">

    <!-- Button backlight -->
    <integer name="config_buttonBrightnessSettingDefault">0</integer>
    <bool name="config_deviceHasVariableButtonBrightness">false</bool>

    <!-- Whether device ignores the RSSNR signal implementation -->
    <bool name="config_ignoreRssnrSignalLevel">true</bool>

    <!-- Whether device has a physical display cutout -->
    <bool name="config_physicalDisplayCutout">true</bool>

    <!-- System info -->
    <string name="config_sysCPUTemp">/sys/class/thermal/thermal_zone0/temp</string>
    <string name="config_sysBatteryTemp">/sys/class/power_supply/battery/temp</string>
    <integer name="config_sysCPUTempMultiplier">1000</integer>
    <integer name="config_sysBatteryTempMultiplier">10</integer>

    <!-- Fingerprint from a certified device against current platform SPL level -->
    <string name="config_certifiedFingerprint" translatable="false">google/walleye/walleye:8.1.0/OPM1.171019.011/4448085:user/release-keys</string>

    <!-- Model from a certified device -->
    <string name="config_certifiedModel" translatable="false">Pixel 2</string>
</resources>

